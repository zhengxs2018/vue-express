{
  "globalStyle": {
    "navigationBarTextStyle": "black",
    "navigationBarTitleText": "uni-app",
    "navigationBarBackgroundColor": "#F8F8F8",
    "backgroundColor": "#F8F8F8"
  },
  "pages": [
    {
      "path": "pages/home/index",
      "style": {
        "navigationBarTitleText": "观看为你推荐的热门视频 | TikTok"
      }
    },
    {
      "path": "pages/discover/index",
      "style": {
        "navigationBarTitleText": "发现"
      }
    },
    {
      "path": "pages/inbox/index",
      "style": {
        "navigationBarTitleText": "消息"
      }
    },
    {
      "path": "pages/mine/index",
      "style": {
        "navigationBarTitleText": "我的"
      }
    }
  ],
  "tabBar": {
    "color": "#393939",
    "selectedColor": "#fff",
    "borderStyle": "black",
    "backgroundColor": "#020202",
    "list": [
      {
        "pagePath": "pages/home/index",
        "iconPath": "./static/icons/home.svg",
        "text": "首页"
      },
      {
        "pagePath": "pages/discover/index",
        "iconPath": "./static/icons/discover.svg",
        "text": "发现"
      },
      {
        "pagePath": "pages/inbox/index",
        "iconPath": "./static/icons/inbox.svg",
        "text": "消息"
      },
      {
        "pagePath": "pages/mine/index",
        "iconPath": "./static/icons/profile.svg",
        "text": "我的"
      }
    ],
    "midButton": {
      "width": "79px",
      "iconPath": "./static/icons/create.svg",
      "iconWidth": "49px"
    }
  },
  "subPackages": [
    {
      "root": "live",
      "pages": [
        {
          "path": "index/index",
          "style": {
            "navigationBarTitleText": "直播间"
          }
        }
      ]
    },
    {
      "root": "shop",
      "pages": [
        {
          "path": "index/index",
          "style": {
            "navigationBarTitleText": "店铺"
          }
        }
      ]
    }
  ],
  "preloadRule": {
    "live/index/index": {
      "network": "all",
      "packages": [
        "__APP__"
      ]
    }
  },
  "easycom": {
    "autoscan": true,
    "custom": {
      "^uni-(.*)": "@dcloudio/uni-ui/lib/uni-$1/uni-$1.vue",
      "^tt-(.*)": "@zhengxs/uni-tiktok/es/components/tt-$1.vue",
      "^ux-(.*)": "@/components/ux-$1.vue"
    }
  }
}
